        ]
    },
    "{N} Grid Layout 3x3 Automatic Width/Height": {
        "prefix": "nsgrid_3x3_auto",
        "body": [
            "<GridLayout rows=\"auto, auto, auto\" columns=\"auto, auto, auto\">",
